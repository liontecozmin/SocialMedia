@{
    ViewData["Title"] = "Manage your emails";
}

<h2>@ViewData["Title"]</h2>
<div>
    <hr />
    <dl class="dl-horizontal">
        

        <dt>Send Emails:</dt>
        <dd>
            <a asp-controller="Account" asp-action="AddEmail" class="btn-bracketed">Send</a>
        </dd>

        <dt>View Emails:</dt>
        <dd>
            <a asp-controller="Account" asp-action="ViewEmail" class="btn-bracketed">View</a>
        </dd>

    </dl>
</div>